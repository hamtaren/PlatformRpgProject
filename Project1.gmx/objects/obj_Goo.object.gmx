    if (obj_Hero.x &lt; x)
    {
        if (state == STATE_MOVE)
        {
            x-=1;
            image_speed = 0.3;
        }
        
        image_xscale = -1;
    }
    else
    {
        if (state == STATE_MOVE)
        {
            x+=1;
            image_speed = 0.3;
        }
        
        image_xscale = 1;
    }    
    
    
    if (state == STATE_SHOOT)
    {
        image_speed = 0;
        image_index = 0;
        
        if (!alarm[0])
        {
            alarm[0] = 45;
            var goo = instance_create(x,y-8,obj_GooProjectile);
            goo.direction = point_direction(x,y,obj_Hero.x,obj_Hero.y-32);
            goo.dmgObject = dmgObject;
        }
    }
    
    if (!collision_line(x,y-4,obj_Hero.x,obj_Hero.y-4,obj_Solid,1,1))
    {
        if (distance_to_object(obj_Hero) &lt; 60)
        {
            state = STATE_SHOOT;
        }
        else if (distance_to_object(obj_Hero) &lt; 200)
        {
            state = STATE_MOVE;
        }    
    }
    else
    {
        state = STATE_NONE;
    }
    
    //Umieranie
    if (hp&lt;=0)
    {
        if (dead == false)
        {
        dead = true;
        sprite_index = spr_GooDeath;
        image_index = 0;
        }
    }
}
else
{
    image_speed = 0.2;
    if (sprite_index == spr_GooDeath &amp;&amp; round(image_index) &gt; image_number - 1)
    {
        instance_destroy();
    }
}


