if (alpha&gt;0)
{
    //NAZWA
    draw_set_halign(fa_center);
    draw_text_bordered(276,2,name);
    
    //ODPORNOSCI
    
    //Tworzenie zmiennych do przechowywania wartosci aktualnie noszonej zbroi
    var eqPierce = 0, eqSlash = 0, eqBlunt = 0, eqFire = 0, eqCold = 0, eqElectro = 0;
    var eqWeight = 0,  eqAttackSpeed =0;
    
    var eqWpn = obj_Hero.objEq.melee[obj_Hero.objEq.mSel];  //noszona bron
    if (instance_exists(eqWpn))                             //czy nosi bron
    {
        eqPierce = eqWpn.pierce;
        eqSlash = eqWpn.slash;
        eqBlunt = eqWpn.blunt;
        eqFire = eqWpn.fire;
        eqCold = eqWpn.cold;
        eqElectro = eqWpn.electro;
        
        eqWeight = eqWpn.weight;
        
        eqAttackSpeed = eqWpn.attackSpeed;
    }
    draw_set_halign(fa_left);
    //ODPORNOSCI FIZYCZNE
    draw_text_bordered_color(200,14,'Kł. '+string(pierce),scr_ItemCompareColor(eqPierce,pierce));
    draw_text_bordered_color(200,26,'Si. '+string(slash),scr_ItemCompareColor(eqSlash,slash));
    draw_text_bordered_color(200,38,'Ob. '+string(blunt),scr_ItemCompareColor(eqBlunt,blunt));
    //ODPORNOSCI ELEMENTALNE
    draw_text_bordered_color(280,14,'Og. '+string(fire),scr_ItemCompareColor(eqFire,fire));
    draw_text_bordered_color(280,26,'Zi. '+string(cold),scr_ItemCompareColor(eqCold,cold));
    draw_text_bordered_color(280,38,'El. '+string(electro),scr_ItemCompareColor(eqElectro,electro));
    //CIEZAR BRONI
    draw_text_bordered_color(200,50,'Waga '+string(weight*2)+'kg',scr_ItemCompareColor(-eqWeight,-weight));
    draw_text_bordered_color(280,50,'Szyb '+string(attackSpeed),scr_ItemCompareColor(eqAttackSpeed,attackSpeed));
    //BUNUSY
    
}
draw_set_alpha(1);
</string>
